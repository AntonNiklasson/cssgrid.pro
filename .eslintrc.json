{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-nested-ternary": 0,

    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": [2, { "skipUndeclared": true }],

    "import/prefer-default-export": 0,

    "jsx-a11y/accessible-emoji": 0
  }
}
