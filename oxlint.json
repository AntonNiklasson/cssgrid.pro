{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/npm/oxlint/configuration_schema.json",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "build",
    "src/views/**",
    "src/components/*.js",
    "src/*.js",
    "*.js"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
